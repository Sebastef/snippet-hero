{
  "name": "snippet-hero",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx ./",
    "watch:lint": "watch 'npm run lint' ./ -d -u",
    "clear": "rm -rf build",
    "build:js": "webpack",
    "watch:js": "npm run build:js -- --watch",
    "build:css": "node-sass client/css/app.scss build/app.css",
    "watch:css": "watch 'npm run build:css -s' client/css/ -d -u",
    "build": "npm run clear -s && npm run build:css -s && npm run build:js -s",
    "watch": "npm-run-all --parallel watch:css watch:js",
    "livereload": "live-reload build/ --port=9091 --delay=400",
    "test:client": "karma start",
    "test:client:firefox": "karma start --browsers Firefox",
    "test:server": "NODE_ENV=test mocha server/test --opts server/test/mocha.opts",
    "test": "npm run test:client:firefox && npm run test:server",
    "server": "nodemon -L ./bin/www",
    "start": "npm-run-all build --parallel watch server livereload",
    "sequelize": "cd server && sequelize",
    "kill": "killall node"
  },
  "dependencies": {
    "alt": "^0.17.9",
    "axios": "^0.7.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.12.1",
    "history": "~1.13.1",
    "jade": "~1.11.0",
    "lodash": "^3.10.1",
    "markdown-react-js": "^0.3.0",
    "material-ui": "^0.13.4",
    "moment": "^2.10.6",
    "morgan": "~1.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "react": "^0.14.6",
    "rand-token": "^0.2.1",
    "react-addons-update": "^0.14.3",
    "react-codemirror": "^0.2.2",
    "react-dom": "^0.14.3",
    "react-router": "1.0.2",
    "react-tap-event-plugin": "^0.2.1",
    "request": "^2.67.0",
    "sequelize": "^3.13.0",
    "sequelize-cli": "^2.1.0",
    "serve-favicon": "~2.3.0",
    "winston": "^2.1.1",
    "winston-daily-rotate-file": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-runtime": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "factory-girl": "^2.2.1",
    "factory-girl-sequelize": "^1.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.6.1",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "live-reload": "^1.1.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "npm-run-all": "^1.2.13",
    "passport-stub": "^1.0.1",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.6",
    "react-addons-test-utils": "^0.14.2",
    "shipit-cli": "^1.4.1",
    "shipit-deploy": "^2.1.2",
    "shipit-pm2": "0.0.4",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0",
    "supertest-session": "^2.0.0",
    "uglify-loader": "^1.3.0",
    "watch": "^0.16.0",
    "webpack": "^1.12.6"
  }
}
